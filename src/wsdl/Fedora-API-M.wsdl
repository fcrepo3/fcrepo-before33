      <input message="ModifyDatastreamExternalRequest"/>
    </operation>
    <operation name="ModifyDatastreamManagedContent">
      <documentation>modifies a Repository-Managed Content Datastream in the 
      object.</documentation>
      <input message="ModifyDatastreamManagedContentRequest"/>
    </operation>
    <operation name="ModifyDatastreamXMLMetadata">
