  <portType name="Fedora-API-M-PortType">
    <operation name="CreateObject">
      <documentation>creates a new, empty digital object in the repository. The 
      object's initial state will be N.  The repository will generate and return 
      a new PID for the object resulting from this request.  The PID will have 
      the namespace of the repository.</documentation>
	  <output message="CreateObjectResponse"/>
    </operation>
    <operation name="IngestObject">
      <documentation>creates a new digital object in the repository, given the 
      data in the provided METS document.  The object's initial state will be N.  
      If the METS document does not specify a PID in the OBJID attribute of the 
      root element, the repository will generate and return a new PID for the 
      object resulting from this request.  That PID will have the namespace of 
      the repository.  If the METS document specifies a PID, it will be assigned 
      to the digital object provided that 1) it conforms to the Fedora PID 
      Syntax, 2) it does not specify the namespace of the repository, and 3) 
      it does not collide with an existing PID of an object in the repository.
      </documentation>
	  <input message="IngestObjectRequest"/>
	  <output message="IngestObjectResponse"/>
    </operation>
    <operation name="GetObjectXML">
      <documentation>gets the XML portion of the entire METS-encoded 
      digital object for external use (viewing, editing, moving to another 
      repository).  XML metadata datastreams will be included inline, content 
      datastreams will not be included, and external datastreams will be 
      referenced by url.</documentation>
      <input message="GetObjectXMLRequest"/>
      <output message="GetObjectXMLResponse"/>
    </operation>
    <operation name="ExportObject">
      <documentation>gets the entire METS-encoded digital object for 
      external use (viewing, editing, moving to another repository).  XML 
      metadata datastreams will be included inline, content datastreams will 
      be included inline (base64-encoded), and external datastreams will be 
      referenced by url.</documentation>
      <input message="ExportObjectRequest"/>
      <output message="ExportObjectResponse"/>
    </operation>
    <operation name="WithdrawObject">
      <documentation>changes the object's state to W, making it inaccessible 
      from API-A.</documentation>
	  <input message="WithdrawObjectRequest"/>
    </operation>
    <operation name="DeleteObject">
      <documentation>marks an object as deleted, changing its state to D.  
      This makes it inaccessible from API-A, and eligible for purging by the 
      cleanup process.</documentation>
      <input message="DeleteObjectRequest"/>
    </operation>
    <operation name="PurgeObject">
      <documentation>permanently removes an object from the repository.
      </documentation>
      <input message="PurgeObjectRequest"/>
    </operation>
    <operation name="ObtainLock">
      <documentation>locks an object for writing.  Upon obtaining a lock, a 
      user may execute write methods on an object.</documentation>
      <input message="ObtainLockRequest"/>
	 </operation>
    <operation name="ReleaseLock">
      <documentation>unlocks an object for writing.</documentation>
      <input message="ReleaseLockRequest"/>
    </operation>
    <operation name="GetLockingUser">
      <documentation>tells who has a write lock on an object.</documentation>
      <input message="GetLockingUserRequest"/>
      <output message="GetLockingUserResponse"/>
    </operation>
    <operation name="GetObjectState">
      <documentation>gets the state of the object.</documentation>
      <input message="GetObjectStateRequest"/>
      <output message="GetObjectStateResponse"/>
    </operation>
    <operation name="GetObjectCreateDate">
      <documentation>gets the date/time of the object's creation in this 
      repository.</documentation>
      <input message="GetObjectCreateDateRequest"/>
      <output message="GetObjectCreateDateResponse"/>
    </operation>
    <operation name="GetObjectLastModDate">
      <documentation>gets the date/time of the object's most recent modification 
      in this repository.  The modification date of an object is set when changes
      to an object are committed via the ReleaseLock method.
      </documentation>
      <input message="GetObjectLastModDateRequest"/>
      <output message="GetObjectLastModDateResponse"/>
    </operation>
    <operation name="GetObjectAuditTrail">
      <documentation>gets all audit trail records for the object.</documentation>
      <input message="GetObjectAuditTrailRequest"/>
      <output message="GetObjectAuditTrailResponse"/>
    </operation>
    <operation name="PurgeObject">
      <documentation>permanently removes the object.</documentation>
      <input message="PurgeObjectRequest"/>
    </operation>
    <operation name="ListObjectPIDs">
      <documentation>gets a list of the PIDs of the objects in the 
      repository.</documentation>
      <input message="ListObjectPIDsRequest"/>
      <output message="ListObjectPIDsResponse"/>
    </operation>
    <operation name="AddDatastreamExternal">
      <documentation>creates a Referenced External Content Datastream in the 
      object.  The MIME type is determined by the repository, which uses the 
      value of the Content-Type HTTP response header provided by an HTTP GET 
      request of the URL.</documentation>
      <input message="AddDatastreamExternalRequest"/>
      <output message="AddDatastreamExternalResponse"/>
    </operation>
    <operation name="AddDatastreamManagedContent">
      <documentation>creates a Repository-Managed Content Datastream in the 
      object.  The repository uses the value of dsContent as the content of 
      the datastream.</documentation>
      <input message="AddDatastreamManagedContentRequest"/>
      <output message="AddDatastreamManagedContentResponse"/>
    </operation>
    <operation name="AddDatastreamXMLMetadata">
      <documentation>creates an Implementor-Defined XML Metadata Datastream in 
      the object.</documentation>
      <input message="AddDatastreamXMLMetadataRequest"/>
      <output message="AddDatastreamXMLMetadataResponse"/>
    </operation>
    <operation name="AddDisseminator">
      <documentation>creates a disseminator in the object.</documentation>
      <input message="AddDisseminatorRequest"/>
      <output message="AddDisseminatorResponse"/>
    </operation>
    <operation name="ModifyDisseminator">
