	<message name="addDatastreamRequest">
		<part name="PID" type="xsd:string">
			<documentation>the PID of the object.</documentation>
		</part>
		<part name="label" type="xsd:string">
			<documentation>the label for the datastream.</documentation>
		</part>
		<part name="MIMEType" type="xsd:string">
			<documentation>the mime type.</documentation>
		</part>
		<part name="location" type="xsd:string">
			<documentation>the location of the datastream.  The content will be retrieved from this location.</documentation>
		</part>
		<part name="controlGroup" type="xsd:string">
			<documentation>the control group.  This must be one of "X", "M", "R", or "E".</documentation>
		</part>
		<part name="MDClass" type="xsd:string">
			<documentation>the metadata class.  This is only required if the controlGroup is "X", and it must be one of "descriptive", "digital provenance", "source", "rights", or "technical".</documentation>
		</part>
		<part name="MDType" type="xsd:string">
			<documentation>the metadata type.  This is only required if the controlGroup is "X", and it should be a well-accepted string that indicates the type of metadata.</documentation>
		</part>
		<part name="dsState" type="xsd:string">
			<documentation>the initial state of the datastream.</documentation>
		</part>
	</message>
	<message name="addDatastreamResponse">
		<part name="response" type="xsd:string">
			<documentation>the datastreamID of the newly added datastream.</documentation>
		</part>
	</message>
	<message name="addDisseminatorRequest">
		<part name="PID" type="xsd:string">
			<documentation>the PID of the object.</documentation>
		</part>
		<part name="bDefPID" type="xsd:string">
			<documentation>the PID of the behavior definition object.</documentation>
		</part>
		<part name="bMechPID" type="xsd:string">
			<documentation>the PID of the behavior mechanism object.</documentation>
		</part>
		<part name="dissLabel" type="xsd:string">
			<documentation>the label of the disseminator.</documentation>
		</part>
		<part name="bDefLabel" type="xsd:string">
			<documentation>the label assigned to the behavior definition of the disseminator.</documentation>
		</part>
		<part name="bMechLabel" type="xsd:string">
			<documentation>the label assigned to the behavior mechanism of the disseminator.</documentation>
		</part>
		<part name="bindingMap" type="fedora-types:DatastreamBindingMap">
			<documentation>the binding map.</documentation>
		</part>
		<part name="dissState" type="xsd:string">
			<documentation>the initial state of the disseminator.</documentation>
		</part>
	</message>
	<message name="addDisseminatorResponse">
		<part name="response" type="xsd:string">
			<documentation>the ID of the newly created disseminator.</documentation>
		</part>
	</message>
	<message name="describeUserRequest">
		<part name="id" type="xsd:string">
			<documentation>the id of the user.</documentation>
		</part>
    </message>
	<message name="describeUserResponse">
		<part name="response" type="fedora-types:UserInfo">
			<documentation>information about the user.</documentation>
