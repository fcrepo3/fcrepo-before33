<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">
<web-app>
	<display-name>ImageManipulation</display-name>
      <servlet>
            <servlet-name>ImageManipulation</servlet-name>
            <display-name>Image Manipulation Servlet</display-name>
            <servlet-class>
                    fedora.localservices.imagemanip.ImageManipulation
            </servlet-class>
      </servlet>
	<servlet-mapping>
		<servlet-name>ImageManipulation</servlet-name>
		<url-pattern>/ImageManipulation/*</url-pattern>
	</servlet-mapping>
</web-app>
